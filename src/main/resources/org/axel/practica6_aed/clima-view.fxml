<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="722.0" prefWidth="808.0" xmlns="http://javafx.com/javafx/23.0.1"
      fx:controller="org.axel.practica6_aed.ControladorClima">
    <children>

        <BarChart fx:id="tiempoEjecucionChart" animated="false" prefHeight="251.0" prefWidth="808.0"
                  title="Tiempo de Ejecución por Algoritmo">
            <xAxis>
                <CategoryAxis label="Algoritmo de Ordenamiento" side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis label="Tiempo (ms)" side="LEFT"/>
            </yAxis>
        </BarChart>
        <ScrollPane prefHeight="166.0" prefWidth="808.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="139.0" prefWidth="1411.0">
                    <children>
                        <HBox>
                            <children>
                                <BarChart fx:id="temperaturaRealChart" alternativeColumnFillVisible="true"
                                          animated="false" legendVisible="false" prefHeight="111.0" prefWidth="196.0"
                                          title="Temperatura real">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="temperaturaAparenteChart" alternativeColumnFillVisible="true"
                                          animated="false" layoutX="10.0" layoutY="10.0" legendVisible="false"
                                          prefHeight="111.0" prefWidth="196.0" title="Temperatura aparente">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="humedadChart" alternativeColumnFillVisible="true" animated="false"
                                          layoutX="210.0" layoutY="10.0" legendVisible="false" prefHeight="111.0"
                                          prefWidth="196.0" title="Humedad">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="velocidadVientoChart" alternativeColumnFillVisible="true"
                                          animated="false" layoutX="410.0" layoutY="10.0" legendVisible="false"
                                          prefHeight="111.0" prefWidth="196.0" title="Velocidad del viento">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="rodamientoVientoChart" alternativeColumnFillVisible="true"
                                          animated="false" layoutX="610.0" layoutY="10.0" legendVisible="false"
                                          prefHeight="111.0" prefWidth="196.0" title="Rodamiento del viento">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="visibilidadChart" alternativeColumnFillVisible="true" animated="false"
                                          layoutX="810.0" layoutY="10.0" legendVisible="false" prefHeight="111.0"
                                          prefWidth="196.0" title="Visibilidad">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="presionChart" alternativeColumnFillVisible="true" animated="false"
                                          layoutX="1010.0" layoutY="10.0" legendVisible="false" prefHeight="111.0"
                                          prefWidth="196.0" title="Presión">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                            </children>
                        </HBox>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <TableView fx:id="datosTable" prefHeight="231.0" prefWidth="808.0">
            <columns>
                <TableColumn fx:id="temperaturaRealColumn" editable="false" prefWidth="118.39999085664749"
                             sortable="false" text="Temperatura real"/>
                <TableColumn fx:id="temperaturaAparenteColumn" editable="false" minWidth="0.0"
                             prefWidth="137.0667266845703" sortable="false" text="Temperatura aparente"/>
                <TableColumn fx:id="humedadColumn" editable="false" minWidth="0.0" prefWidth="78.13330078125"
                             sortable="false" text="Humedad"/>
                <TableColumn fx:id="velocidadVientoColumn" editable="false" prefWidth="121.33331298828125"
                             sortable="false" text="Velocidad del viento"/>
                <TableColumn fx:id="rodamientoVientoColumn" editable="false" prefWidth="134.93338012695312"
                             sortable="false" text="Rodamiento del viento"/>
                <TableColumn fx:id="visibilidadColumn" editable="false" prefWidth="67.2000732421875" sortable="false"
                             text="Visibilidad"/>
                <TableColumn fx:id="presionColumn" editable="false" prefWidth="52.53338623046875" sortable="false"
                             text="Presión"/>
            </columns>
        </TableView>
        <HBox prefHeight="95.0" prefWidth="808.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="639.0">
                    <children>
                        <VBox prefHeight="90.0" prefWidth="535.0">
                            <children>
                                <HBox prefHeight="18.0" prefWidth="430.0">
                                    <children>
                                        <CheckBox fx:id="quickCheckBox" mnemonicParsing="false" prefHeight="18.0"
                                                  prefWidth="63.0" text="Quick"/>
                                        <CheckBox fx:id="mergeCheckBox" layoutX="10.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Merge"/>
                                        <CheckBox fx:id="shellCheckBox" layoutX="84.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Shell"/>
                                        <CheckBox fx:id="selectionCheckBox" layoutX="159.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Selection"/>
                                        <CheckBox fx:id="radixCheckBox" layoutX="233.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Radix"/>
                                        <CheckBox fx:id="sortCheckBox" layoutX="300.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Sort (Java)"/>
                                        <CheckBox fx:id="parallelCheckBox" layoutX="301.0" layoutY="10.0"
                                                  mnemonicParsing="false" text="Parallel Sort"/>
                                    </children>
                                </HBox>
                                <HBox>
                                    <children>
                                        <Separator disable="true" prefHeight="27.0" prefWidth="167.0" visible="false"/>
                                        <ComboBox fx:id="campoOrdenarComboBox" prefHeight="42.0" prefWidth="143.0"
                                                  promptText="Campo a ordenar">
                                            <opaqueInsets>
                                                <Insets/>
                                            </opaqueInsets>
                                            <HBox.margin>
                                                <Insets top="10.0"/>
                                            </HBox.margin>
                                        </ComboBox>
                                    </children>
                                </HBox>
                            </children>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </VBox>
                        <VBox>
                            <children>
                                <Button fx:id="ordenarButton" alignment="CENTER" contentDisplay="CENTER"
                                        mnemonicParsing="false" onAction="#onOrdenarButtonClick" prefHeight="51.0"
                                        prefWidth="253.0" text="Ordenar">
                                    <font>
                                        <Font size="24.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="randomizarButton" mnemonicParsing="false"
                                        onAction="#onRandomizarButtonClick" prefHeight="26.0" prefWidth="183.0"
                                        text="Randomizar"/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <VBox prefHeight="90.0" prefWidth="169.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="184.0"
                               text="Tiempo total">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="tiempoLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="48.0"
                               prefWidth="184.0" text="0 ms">
                            <font>
                                <Font name="System Italic" size="16.0"/>
                            </font>
                        </Label>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets top="20.0"/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>